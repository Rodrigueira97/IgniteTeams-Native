{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths":{
      "@pages/*":[
        "./src/pages/*"
      ],
      "@assets/*":[
        "./src/assets/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ],
      "@storage/*":[
        "./src/storage/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
      "@theme/*":[
        "./src/theme/*"
      ],
    }
  },
}
